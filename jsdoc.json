{
    "source": {
        "include": [ "src" ],
        "includePattern": ".+\\.jsx?$",
        "excludePattern": "(^|/)node_modules/"
    },
    "plugins": [ "plugins/markdown" ],
    "opts": {
        "recurse": true,
        "destination": "docs"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}